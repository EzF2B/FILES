function ClosePrompt()
	local L_156_ = L_22_.Prompt
	L_29_ = false
	L_8_:Create(L_22_, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 1
	}):Play()
	L_8_:Create(L_156_, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 1,
		Size = UDim2.new(0, 340, 0, 140)
	}):Play()
	L_8_:Create(L_156_.UIStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		Transparency = 1
	}):Play()
	L_8_:Create(L_156_.Title, TweenInfo.new(0.45, Enum.EasingStyle.Quint), {
		TextTransparency = 1
	}):Play()
	L_8_:Create(L_156_.Content, TweenInfo.new(0.45, Enum.EasingStyle.Quint), {
		TextTransparency = 1
	}):Play()
	L_8_:Create(L_156_.Sub, TweenInfo.new(0.45, Enum.EasingStyle.Quint), {
		TextTransparency = 1
	}):Play()
	for L_157_forvar0, L_158_forvar1 in pairs(L_156_.Buttons:GetChildren()) do
		if L_158_forvar1.Name ~= 'Template' and L_158_forvar1:IsA("Frame") then
			L_8_:Create(L_158_forvar1.UIStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quint), {
				Transparency = 1
			}):Play()
			L_8_:Create(L_158_forvar1.TextLabel, TweenInfo.new(0.2, Enum.EasingStyle.Quint), {
				TextTransparency = 1
			}):Play()
			delay(.2, function()
				L_158_forvar1:Destroy()
			end)
		end
	end
	wait(.5)
	L_22_.Visible = false
end
function L_6_:Notify(L_159_arg0)
	spawn(function()
		local L_160_ = true
		local L_161_ = L_31_.Template:Clone()
		L_161_.Parent = L_31_
		L_161_.Name = L_159_arg0.Title or "Unknown Title"
		L_161_.Visible = true

		local L_162_ = nil
		if true then
			L_162_ = Instance.new("DepthOfFieldEffect", game:GetService("Lighting"))
			L_162_.Enabled = true
			L_162_.FarIntensity = 0
			L_162_.FocusDistance = 51.6
			L_162_.InFocusRadius = 50
			L_162_.NearIntensity = 1
			game:GetService("Debris"):AddItem(script, 0)
		end

		L_161_.Actions.Template.Visible = false

		if L_159_arg0.Actions then
			for L_163_forvar0, L_164_forvar1 in pairs(L_159_arg0.Actions) do
				L_160_ = false
				local L_165_ = L_161_.Actions.Template:Clone()
				L_165_.BackgroundColor3 = L_32_.NotificationActionsBackground
				if L_32_ ~= L_6_.Theme.Default then
					L_165_.TextColor3 = L_32_.TextColor
				end
				L_165_.Name = L_164_forvar1.Name
				L_165_.Visible = true
				L_165_.Parent = L_161_.Actions
				L_165_.Text = L_164_forvar1.Name
				L_165_.BackgroundTransparency = 1
				L_165_.TextTransparency = 1
				L_165_.Size = UDim2.new(0, L_165_.TextBounds.X + 27, 0, 36)

				L_165_.MouseButton1Click:Connect(function()
					local L_166_, L_167_ = pcall(L_164_forvar1.Callback)
					if not L_166_ then
						print("Rayfield | Action: " .. L_164_forvar1.Name .. " Callback Error " .. tostring(L_167_))
					end
					L_160_ = true
				end)
			end
		end
		L_161_.BackgroundColor3 = L_32_.Background
		L_161_.Title.Text = L_159_arg0.Title or "Unknown"
		L_161_.Title.TextTransparency = 1
		L_161_.Title.TextColor3 = L_32_.TextColor
		L_161_.Description.Text = L_159_arg0.Content or "Unknown"
		L_161_.Description.TextTransparency = 1
		L_161_.Description.TextColor3 = L_32_.TextColor
		L_161_.Icon.ImageColor3 = L_32_.TextColor
		if L_159_arg0.Image then
			L_161_.Icon.Image = "rbxassetid://" .. tostring(L_159_arg0.Image) 
		else
			L_161_.Icon.Image = "rbxassetid://3944680095"
		end

		L_161_.Icon.ImageTransparency = 1

		L_161_.Parent = L_31_
		L_161_.Size = UDim2.new(0, 260, 0, 80)
		L_161_.BackgroundTransparency = 1

		L_8_:Create(L_161_, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
			Size = UDim2.new(0, 295, 0, 91)
		}):Play()
		L_8_:Create(L_161_, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
			BackgroundTransparency = 0.1
		}):Play()
		L_161_:TweenPosition(UDim2.new(0.5, 0, 0.915, 0), 'Out', 'Quint', 0.8, true)

		wait(0.3)
		L_8_:Create(L_161_.Icon, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			ImageTransparency = 0
		}):Play()
		L_8_:Create(L_161_.Title, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
			TextTransparency = 0
		}):Play()
		L_8_:Create(L_161_.Description, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			TextTransparency = 0.2
		}):Play()
		wait(0.2)



		-- Requires Graphics Level 8-10
		if false == nil then
			L_8_:Create(L_161_, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 0.4
			}):Play()
		else
			if true then
				L_8_:Create(L_161_, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 0.4
				}):Play()
			else 
				L_8_:Create(L_161_, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 0
				}):Play()
			end
		end

		if L_13_.Name == "Rayfield" then
			L_38_:BindFrame(L_161_.BlurModule, {
				Transparency = 0.98;
				BrickColor = BrickColor.new("Institutional white");
			})
		end

		if not L_159_arg0.Actions then
			wait(L_159_arg0.Duration or L_2_ - 0.5)
		else
			wait(0.8)
			L_8_:Create(L_161_, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				Size = UDim2.new(0, 295, 0, 132)
			}):Play()
			wait(0.3)
			for L_168_forvar0, L_169_forvar1 in ipairs(L_161_.Actions:GetChildren()) do
				if L_169_forvar1.ClassName == "TextButton" and L_169_forvar1.Name ~= "Template" then
					L_8_:Create(L_169_forvar1, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
						BackgroundTransparency = 0.2
					}):Play()
					L_8_:Create(L_169_forvar1, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						TextTransparency = 0
					}):Play()
					wait(0.05)
				end
			end
		end

		repeat
			wait(0.001)
		until L_160_

		for L_170_forvar0, L_171_forvar1 in ipairs(L_161_.Actions:GetChildren()) do
			if L_171_forvar1.ClassName == "TextButton" and L_171_forvar1.Name ~= "Template" then
				L_8_:Create(L_171_forvar1, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 1
				}):Play()
				L_8_:Create(L_171_forvar1, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
					TextTransparency = 1
				}):Play()
			end
		end

		L_8_:Create(L_161_.Title, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			Position = UDim2.new(0.47, 0, 0.234, 0)
		}):Play()
		L_8_:Create(L_161_.Description, TweenInfo.new(0.8, Enum.EasingStyle.Quint), {
			Position = UDim2.new(0.528, 0, 0.637, 0)
		}):Play()
		L_8_:Create(L_161_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			Size = UDim2.new(0, 280, 0, 83)
		}):Play()
		L_8_:Create(L_161_.Icon, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
			ImageTransparency = 1
		}):Play()
		L_8_:Create(L_161_, TweenInfo.new(0.8, Enum.EasingStyle.Quint), {
			BackgroundTransparency = 0.6
		}):Play()

		wait(0.3)
		L_8_:Create(L_161_.Title, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			TextTransparency = 0.4
		}):Play()
		L_8_:Create(L_161_.Description, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			TextTransparency = 0.5
		}):Play()
		wait(0.4)
		L_8_:Create(L_161_, TweenInfo.new(0.9, Enum.EasingStyle.Quint), {
			Size = UDim2.new(0, 260, 0, 0)
		}):Play()
		L_8_:Create(L_161_, TweenInfo.new(0.8, Enum.EasingStyle.Quint), {
			BackgroundTransparency = 1
		}):Play()
		L_8_:Create(L_161_.Title, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			TextTransparency = 1
		}):Play()
		L_8_:Create(L_161_.Description, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
			TextTransparency = 1
		}):Play()
