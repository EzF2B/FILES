function ChangeTheme(L_43_arg0)
	L_32_ = L_6_.Theme[L_43_arg0]
	for L_44_forvar0, L_45_forvar1 in ipairs(L_13_:GetDescendants()) do
		if L_45_forvar1.ClassName == "TextLabel" or L_45_forvar1.ClassName == "TextBox" or L_45_forvar1.ClassName == "TextButton" then
			if L_32_.TextFont ~= "Default" then 
				L_45_forvar1.TextColor3 = L_32_.TextColor
				L_45_forvar1.Font = L_32_.TextFont
			end
		end
	end

	L_13_.Main.BackgroundColor3 = L_32_.Background
	L_13_.Main.Topbar.BackgroundColor3 = L_32_.Topbar
	L_13_.Main.Topbar.CornerRepair.BackgroundColor3 = L_32_.Topbar
	L_13_.Main.Shadow.Image.ImageColor3 = L_32_.Shadow

	L_13_.Main.Topbar.ChangeSize.ImageColor3 = L_32_.TextColor
	L_13_.Main.Topbar.Hide.ImageColor3 = L_32_.TextColor
	L_13_.Main.Topbar.Theme.ImageColor3 = L_32_.TextColor

	for L_46_forvar0, L_47_forvar1 in ipairs(L_17_:GetChildren()) do
		for L_48_forvar0, L_49_forvar1 in ipairs(L_47_forvar1:GetChildren()) do
			if L_49_forvar1.ClassName == "Frame" and L_49_forvar1.Name ~= "Placeholder" and L_49_forvar1.Name ~= "SectionSpacing" and L_49_forvar1.Name ~= "SectionTitle" then
				L_49_forvar1.BackgroundColor3 = L_32_.ElementBackground
				L_49_forvar1.UIStroke.Color = L_32_.ElementStroke
			end
		end
	end

