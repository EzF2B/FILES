								L_380_ = L_375_ + L_378_ * 0.5
							elseif L_380_ > L_376_ - L_378_ * 0.5 then
								L_380_ = L_376_ - L_378_ * 0.5
							end

							L_359_ = (L_380_ - L_375_) / L_377_
							L_358_.Hue.Selector.Position = UDim2.new(0.5, - (L_375_ + L_377_ * 0.5 - L_380_), 0.5, 0)
							L_358_.Hue.Show.BackgroundColor3 = Color3.fromHSV(L_359_, 1, 1)
							L_358_.Saturation.BackgroundColor3 = L_358_.Hue.Show.BackgroundColor3
							L_358_.Value.BackgroundColor3 = L_358_.Hue.Show.BackgroundColor3
						else
