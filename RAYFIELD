		-- Keybind
		function L_234_:CreateKeybind(L_319_arg0)
			local L_320_ = false
			local L_321_ = L_17_.Template.Keybind:Clone()
			L_321_.Name = L_319_arg0.Name
			L_321_.Title.Text = L_319_arg0.Name
			L_321_.Visible = true
			L_321_.Parent = L_233_

			L_321_.BackgroundTransparency = 1
			L_321_.UIStroke.Transparency = 1
			L_321_.Title.TextTransparency = 1

