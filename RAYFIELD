		if true then
			L_38_:UnbindFrame(L_161_.BlurModule)
			L_162_:Destroy()
		end
		wait(0.9)
		L_161_:Destroy()
	end)
end

function Hide()
	L_28_ = true
	L_6_:Notify({
		Title = "Interface Hidden",
		Content = "The interface has been hidden, you can unhide the interface by tapping RightShift",
		Duration = 7
	})
	L_8_:Create(L_15_, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Size = UDim2.new(0, 470, 0, 400)
	}):Play()
