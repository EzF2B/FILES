			L_8_:Create(L_175_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				TextTransparency = 1
			}):Play()
			L_8_:Create(L_175_forvar1.Image, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				ImageTransparency = 1
			}):Play()
			L_8_:Create(L_175_forvar1.Shadow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				ImageTransparency = 1
			}):Play()
			L_8_:Create(L_175_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				Transparency = 1
			}):Play()
		end
	end
	for L_176_forvar0, L_177_forvar1 in ipairs(L_17_:GetChildren()) do
		if L_177_forvar1.Name ~= "Template" and L_177_forvar1.ClassName == "ScrollingFrame" and L_177_forvar1.Name ~= "Placeholder" then
			for L_178_forvar0, L_179_forvar1 in ipairs(L_177_forvar1:GetChildren()) do
				if L_179_forvar1.ClassName == "Frame" then
					if L_179_forvar1.Name ~= "SectionSpacing" and L_179_forvar1.Name ~= "Placeholder" then
						if L_179_forvar1.Name == "SectionTitle" then
							L_8_:Create(L_179_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								TextTransparency = 1
							}):Play()
