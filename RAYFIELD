						end
					end)
				end
			end)

			L_358_.Saturation.InputBegan:Connect(function(L_381_arg0)
				if L_381_arg0.UserInputType == Enum.UserInputType.MouseButton1 then
					local L_382_ = L_358_.Saturation.AbsolutePosition.X
					local L_383_ = L_382_ + L_358_.Saturation.AbsoluteSize.X
