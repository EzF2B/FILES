								L_191_forvar1.Visible = true
							end
						end
					end
				end
			end
		end
	end
	wait(0.5)
	L_26_ = false
	L_28_ = false
end
function CloseSearch()
	L_28_ = true
	L_8_:Create(L_20_, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 1,
		Size = UDim2.new(0, 460, 0, 35)
	}):Play()
