				TextTransparency = 0
			}):Play()	

			L_390_.Main.Progress.Size = UDim2.new(0, L_390_.Main.AbsoluteSize.X * ((L_388_arg0.CurrentValue + L_388_arg0.Range[1]) / (L_388_arg0.Range[2] - L_388_arg0.Range[1])) > 5 and L_390_.Main.AbsoluteSize.X * (L_388_arg0.CurrentValue / (L_388_arg0.Range[2] - L_388_arg0.Range[1])) or 5, 1, 0)

			if not L_388_arg0.Suffix then
				L_390_.Main.Information.Text = tostring(L_388_arg0.CurrentValue)
			else
				L_390_.Main.Information.Text = tostring(L_388_arg0.CurrentValue) .. " " .. L_388_arg0.Suffix
			end


			L_390_.MouseEnter:Connect(function()
				L_8_:Create(L_390_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
					BackgroundColor3 = L_32_.ElementBackgroundHover
				}):Play()
			end)

			L_390_.MouseLeave:Connect(function()
				L_8_:Create(L_390_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
					BackgroundColor3 = L_32_.ElementBackground
				}):Play()
			end)

			L_390_.Main.Interact.InputBegan:Connect(function(L_398_arg0)
				if L_398_arg0.UserInputType == Enum.UserInputType.MouseButton1 then 
					L_389_ = true 
				end 
			end)
			L_390_.Main.Interact.InputEnded:Connect(function(L_399_arg0) 
				if L_399_arg0.UserInputType == Enum.UserInputType.MouseButton1 then 
					L_389_ = false 
				end 
			end)

			L_390_.Main.Interact.MouseButton1Down:Connect(function(L_400_arg0)
				local L_401_ = L_390_.Main.Progress.AbsolutePosition.X + L_390_.Main.Progress.AbsoluteSize.X
				local L_402_ = L_401_
				local L_403_ = L_400_arg0
				local L_404_;
				L_404_ = L_10_.Stepped:Connect(function()
					if L_389_ then
						L_403_ = L_7_:GetMouseLocation().X
						L_401_ = L_401_ + 0.025 * (L_403_ - L_402_)

						if L_403_ < L_390_.Main.AbsolutePosition.X then
							L_403_ = L_390_.Main.AbsolutePosition.X
						elseif L_403_ > L_390_.Main.AbsolutePosition.X + L_390_.Main.AbsoluteSize.X then
							L_403_ = L_390_.Main.AbsolutePosition.X + L_390_.Main.AbsoluteSize.X
						end

						if L_401_ < L_390_.Main.AbsolutePosition.X + 5 then
							L_401_ = L_390_.Main.AbsolutePosition.X + 5
						elseif L_401_ > L_390_.Main.AbsolutePosition.X + L_390_.Main.AbsoluteSize.X then
							L_401_ = L_390_.Main.AbsolutePosition.X + L_390_.Main.AbsoluteSize.X
						end

						if L_401_ <= L_403_ and (L_403_ - L_402_) < 0 then
							L_402_ = L_403_
						elseif L_401_ >= L_403_ and (L_403_ - L_402_) > 0 then
							L_402_ = L_403_
						end
						L_8_:Create(L_390_.Main.Progress, TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
							Size = UDim2.new(0, L_401_ - L_390_.Main.AbsolutePosition.X, 1, 0)
						}):Play()
						local L_405_ = L_388_arg0.Range[1] + (L_403_ - L_390_.Main.AbsolutePosition.X) / L_390_.Main.AbsoluteSize.X * (L_388_arg0.Range[2] - L_388_arg0.Range[1])

						L_405_ = math.floor(L_405_ / L_388_arg0.Increment + 0.5) * (L_388_arg0.Increment * 10000000) / 10000000
						if not L_388_arg0.Suffix then
							L_390_.Main.Information.Text = tostring(L_405_)
						else
							L_390_.Main.Information.Text = tostring(L_405_) .. " " .. L_388_arg0.Suffix
						end

						if L_388_arg0.CurrentValue ~= L_405_ then
							local L_406_, L_407_ = pcall(function()
								L_388_arg0.Callback(L_405_)
							end)
							if not L_406_ then
								L_8_:Create(L_390_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
									BackgroundColor3 = Color3.fromRGB(85, 0, 0)
								}):Play()
								L_8_:Create(L_390_.UIStroke, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
									Transparency = 1
								}):Play()
								L_390_.Title.Text = "Callback Error"
								print("Rayfield | " .. L_388_arg0.Name .. " Callback Error " .. tostring(L_407_))
								wait(0.5)
								L_390_.Title.Text = L_388_arg0.Name
								L_8_:Create(L_390_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
									BackgroundColor3 = L_32_.ElementBackground
								}):Play()
								L_8_:Create(L_390_.UIStroke, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
									Transparency = 0
								}):Play()
							end

							L_388_arg0.CurrentValue = L_405_
							L_37_func()
						end
					else
						L_8_:Create(L_390_.Main.Progress, TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
							Size = UDim2.new(0, L_403_ - L_390_.Main.AbsolutePosition.X > 5 and L_403_ - L_390_.Main.AbsolutePosition.X or 5, 1, 0)
						}):Play()
						L_404_:Disconnect()
					end
				end)
			end)

			function L_388_arg0:Set(L_408_arg0)
				L_8_:Create(L_390_.Main.Progress, TweenInfo.new(0.45, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
					Size = UDim2.new(0, L_390_.Main.AbsoluteSize.X * ((L_408_arg0 + L_388_arg0.Range[1]) / (L_388_arg0.Range[2] - L_388_arg0.Range[1])) > 5 and L_390_.Main.AbsoluteSize.X * (L_408_arg0 / (L_388_arg0.Range[2] - L_388_arg0.Range[1])) or 5, 1, 0)
				}):Play()
				L_390_.Main.Information.Text = tostring(L_408_arg0) .. " " .. L_388_arg0.Suffix
				local L_409_, L_410_ = pcall(function()
					L_388_arg0.Callback(L_408_arg0)
				end)
				if not L_409_ then
					L_8_:Create(L_390_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						BackgroundColor3 = Color3.fromRGB(85, 0, 0)
					}):Play()
					L_8_:Create(L_390_.UIStroke, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						Transparency = 1
					}):Play()
					L_390_.Title.Text = "Callback Error"
					print("Rayfield | " .. L_388_arg0.Name .. " Callback Error " .. tostring(L_410_))
					wait(0.5)
					L_390_.Title.Text = L_388_arg0.Name
					L_8_:Create(L_390_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						BackgroundColor3 = L_32_.ElementBackground
					}):Play()
					L_8_:Create(L_390_.UIStroke, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						Transparency = 0
					}):Play()
				end
				L_388_arg0.CurrentValue = L_408_arg0
				L_37_func()
			end
			if L_214_arg0.ConfigurationSaving then
				if L_214_arg0.ConfigurationSaving.Enabled and L_388_arg0.Flag then
					L_6_.Flags[L_388_arg0.Flag] = L_388_arg0
				end
			end
			return L_388_arg0
		end


		return L_234_
	end

	L_17_.Visible = true

	wait(1.2)
	L_8_:Create(L_18_.Title, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
