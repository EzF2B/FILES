			L_358_.Saturation.Selector.Position = UDim2.new(0.5, L_368_, 0.5, 0)

			L_358_.Hue.Show.BackgroundColor3 = Color3.fromHSV(L_359_, 1, 1)
			L_358_.Value.BackgroundColor3 = Color3.fromHSV(L_359_, 1, 1)
			L_358_.Value.Show.BackgroundColor3 = Color3.fromHSV(L_359_, L_360_, 1)
			L_358_.Saturation.BackgroundColor3 = Color3.fromHSV(L_359_, 1, 1)
			L_358_.Saturation.Show.BackgroundColor3 = Color3.fromHSV(L_359_, 1, L_361_)

			L_358_.Hue.InputBegan:Connect(function(L_374_arg0)
				if L_374_arg0.UserInputType == Enum.UserInputType.MouseButton1 then
					local L_375_ = L_358_.Hue.AbsolutePosition.X
					local L_376_ = L_375_ + L_358_.Hue.AbsoluteSize.X
					local L_377_ = L_358_.Hue.AbsoluteSize.X
					local L_378_ = L_358_.Hue.Selector.AbsoluteSize.X

					local L_379_;
					L_379_ = L_10_.RenderStepped:Connect(function()
						if L_7_:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) then
							local L_380_ = L_7_:GetMouseLocation().X

