		 --Colorpicker
		function L_234_:CreateColorpicker(L_355_arg0)
			local L_356_ = L_17_.Template.Color:Clone()
			L_356_.Name = L_355_arg0.Name
			L_356_.Title.Text = L_355_arg0.Name
			L_356_.Visible = true
			L_356_.Parent = L_233_
			local L_357_ = L_355_arg0.CurrentColor
			local L_358_ = L_356_.Main
			local L_359_, L_360_, L_361_ = L_355_arg0.CurrentColor:ToHSV()
			
			local function L_362_func(L_369_arg0, L_370_arg1, L_371_arg2)
				local L_372_ = L_371_arg2
				local L_373_ = L_371_arg2 + L_370_arg1

				if L_369_arg0 + L_372_ < L_372_ + 3 then
					L_369_arg0 = L_372_ + 3
				elseif L_369_arg0 + L_372_ > L_373_ - 3 then
					L_369_arg0 = L_373_ - 3
				end
				
				L_369_arg0 = L_371_arg2 + L_369_arg0
				return L_369_arg0, - (L_372_ + L_370_arg1 * 0.5 - L_369_arg0)
			end
			
			local L_363_, L_364_ = L_362_func(L_358_.Hue.Selector.AbsoluteSize.X * L_359_, L_358_.Hue.Selector.AbsoluteSize.X, L_358_.Hue.Selector.AbsolutePosition.X)
			local L_365_, L_366_ = L_362_func(L_358_.Hue.Selector.AbsoluteSize.X * L_360_, L_358_.Hue.Selector.AbsoluteSize.X, L_358_.Hue.Selector.AbsolutePosition.X)
			local L_367_, L_368_ = L_362_func(L_358_.Hue.Selector.AbsoluteSize.X * L_361_, L_358_.Hue.Selector.AbsoluteSize.X, L_358_.Hue.Selector.AbsolutePosition.X)

			L_358_.Hue.Selector.Position = UDim2.new(0.5, - (L_358_.Hue.Selector.AbsolutePosition.X + L_358_.Hue.Selector.AbsoluteSize.X * 0.5 - L_363_), 0.5, 0)
