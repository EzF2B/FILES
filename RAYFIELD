function Unhide()
	L_28_ = true
	L_15_.Position = UDim2.new(0.5, 0, 0.5, 0)
	L_15_.Visible = true
	L_8_:Create(L_15_, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Size = UDim2.new(0, 500, 0, 475)
	}):Play()
	L_8_:Create(L_15_.Topbar, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Size = UDim2.new(0, 500, 0, 45)
	}):Play()
	L_8_:Create(L_15_.Shadow.Image, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
		ImageTransparency = 0.4
	}):Play()
	L_8_:Create(L_15_, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 0
	}):Play()
	L_8_:Create(L_15_.Topbar, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 0
	}):Play()
	L_8_:Create(L_15_.Topbar.Divider, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 0
	}):Play()
	L_8_:Create(L_15_.Topbar.CornerRepair, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 0
	}):Play()
	L_8_:Create(L_15_.Topbar.Title, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		TextTransparency = 0
	}):Play()
	if L_26_ then
		spawn(Maximise)
	end
	for L_182_forvar0, L_183_forvar1 in ipairs(L_16_:GetChildren()) do
		if L_183_forvar1.ClassName == "ImageButton" then
			L_8_:Create(L_183_forvar1, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				ImageTransparency = 0.8
			}):Play()
		end
	end
	for L_184_forvar0, L_185_forvar1 in ipairs(L_19_:GetChildren()) do
		if L_185_forvar1.ClassName == "Frame" and L_185_forvar1.Name ~= "Placeholder" then
			if tostring(L_17_.UIPageLayout.CurrentPage) == L_185_forvar1.Title.Text then
				L_8_:Create(L_185_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 0
				}):Play()
				L_8_:Create(L_185_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					TextTransparency = 0
				}):Play()
				L_8_:Create(L_185_forvar1.Shadow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0.9
				}):Play()
				L_8_:Create(L_185_forvar1.Image, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0
				}):Play()
				L_8_:Create(L_185_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					Transparency = 1
				}):Play()
			else
				L_8_:Create(L_185_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 0.7
				}):Play()
				L_8_:Create(L_185_forvar1.Image, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0.2
				}):Play()
				L_8_:Create(L_185_forvar1.Shadow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0.7
				}):Play()
				L_8_:Create(L_185_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					TextTransparency = 0.2
				}):Play()
				L_8_:Create(L_185_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					Transparency = 0
				}):Play()
			end

		end
	end
	for L_186_forvar0, L_187_forvar1 in ipairs(L_17_:GetChildren()) do
		if L_187_forvar1.Name ~= "Template" and L_187_forvar1.ClassName == "ScrollingFrame" and L_187_forvar1.Name ~= "Placeholder" then
			for L_188_forvar0, L_189_forvar1 in ipairs(L_187_forvar1:GetChildren()) do
				if L_189_forvar1.ClassName == "Frame" then
					if L_189_forvar1.Name ~= "SectionSpacing" and L_189_forvar1.Name ~= "Placeholder" then
						if L_189_forvar1.Name == "SectionTitle" then
							L_8_:Create(L_189_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								TextTransparency = 0
							}):Play()
						else
							L_8_:Create(L_189_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								BackgroundTransparency = 0
							}):Play()
							L_8_:Create(L_189_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								Transparency = 0
							}):Play()
							L_8_:Create(L_189_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								TextTransparency = 0
							}):Play()
						end
						for L_190_forvar0, L_191_forvar1 in ipairs(L_189_forvar1:GetChildren()) do
