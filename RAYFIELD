local function L_33_func(L_50_arg0, L_51_arg1)
	pcall(function()
		local L_52_, L_53_, L_54_, L_55_ = false, false, false, false
		L_50_arg0.InputBegan:Connect(function(L_56_arg0)
			if L_56_arg0.UserInputType == Enum.UserInputType.MouseButton1 then
				L_52_ = true
				L_54_ = L_56_arg0.Position
				L_55_ = L_51_arg1.Position

				L_56_arg0.Changed:Connect(function()
					if L_56_arg0.UserInputState == Enum.UserInputState.End then
						L_52_ = false
					end
				end)
			end
		end)
		L_50_arg0.InputChanged:Connect(function(L_57_arg0)
			if L_57_arg0.UserInputType == Enum.UserInputType.MouseMovement then
				L_53_ = L_57_arg0
			end
		end)
		L_7_.InputChanged:Connect(function(L_58_arg0)
			if L_58_arg0 == L_53_ and L_52_ then
				local L_59_ = L_58_arg0.Position - L_54_
				L_8_:Create(L_51_arg1, TweenInfo.new(0.45, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
					Position = UDim2.new(L_55_.X.Scale, L_55_.X.Offset + L_59_.X, L_55_.Y.Scale, L_55_.Y.Offset + L_59_.Y)
				}):Play()
			end
		end)
	end)
end   

local function L_34_func(L_60_arg0)
	return {
		R = L_60_arg0.R * 255,
		G = L_60_arg0.G * 255,
		B = L_60_arg0.B * 255
	}
end    

local function L_35_func(L_61_arg0)
	return Color3.fromRGB(L_61_arg0.R, L_61_arg0.G, L_61_arg0.B)
end

local function L_36_func(L_62_arg0)
	local L_63_ = L_9_:JSONDecode(L_62_arg0)
	table.foreach(L_63_, function(L_64_arg0, L_65_arg1)
		if L_6_.Flags[L_64_arg0] then
			spawn(function() 
				if L_6_.Flags[L_64_arg0].Type == "Colorpicker" then
					L_6_.Flags[L_64_arg0]:Set(L_35_func(L_65_arg1))
				else
					if L_6_.Flags[L_64_arg0].CurrentValue or L_6_.Flags[L_64_arg0].CurrentKeybind or L_6_.Flags[L_64_arg0].CurrentOption ~= L_65_arg1 then
						L_6_.Flags[L_64_arg0]:Set(L_65_arg1)
					end
				end    
			end)
		else
			L_6_:Notify({
				Title = "Flag Error",
				Content = "Rayfield was unable to find '" .. L_64_arg0 .. "'' in the current script"
			})
		end
	end)
end

local function L_37_func()
	if not L_25_ then
		return
	end
	local L_66_ = {}
	for L_67_forvar0, L_68_forvar1 in pairs(L_6_.Flags) do
		if L_68_forvar1.Type == "Colorpicker" then
			L_66_[L_67_forvar0] = L_34_func(L_68_forvar1.CurrentValue)
		else
			L_66_[L_67_forvar0] = L_68_forvar1.CurrentValue or L_68_forvar1.CurrentKeybind or L_68_forvar1.CurrentOption
		end
	end	
	writefile(L_4_ .. "/" .. L_24_ .. L_5_, tostring(L_9_:JSONEncode(L_66_)))
end

