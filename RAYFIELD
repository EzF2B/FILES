	function L_217_:Prompt(L_411_arg0)
		local L_412_ = L_22_.Prompt
		L_22_.Visible = true
		L_22_.BackgroundTransparency = 1
		L_412_.BackgroundTransparency = 1
		L_412_.UIStroke.Transparency = 1
		L_412_.Content.TextTransparency = 1
		L_412_.Title.TextTransparency = 1
		L_412_.Sub.TextTransparency = 1
		L_412_.Size = UDim2.new(0, 340, 0, 140)
		L_412_.Buttons.Template.Visible = false
		L_412_.Buttons.Template.TextLabel.TextTransparency = 1
		L_412_.Buttons.Template.UIStroke.Transparency = 1
		--PromptUI.Buttons.Middle.Visible = false
		--PromptUI.Buttons.Middle.TextLabel.TextTransparency = 1
		--PromptUI.Buttons.Middle.UIStroke.Transparency = 1
		
		L_412_.Content.Text = L_411_arg0.Content
		L_412_.Sub.Text = L_411_arg0.SubTitle or ''
		
		if L_411_arg0.Actions then
			for L_413_forvar0, L_414_forvar1 in pairs(L_411_arg0.Actions) do
				print(L_414_forvar1)
				local L_415_ = L_412_.Buttons.Template:Clone()
				L_415_.TextLabel.Text = L_414_forvar1.Name
				L_415_.Interact.MouseButton1Up:Connect(function()
					if not L_29_ then
						local L_416_, L_417_ = pcall(L_414_forvar1.Callback)
						L_29_ = true
						if not L_416_ then
							ClosePrompt()
							print("Rayfield | " .. L_414_forvar1.Name .. " Callback Error " .. tostring(L_417_))
						else
							ClosePrompt()
						end
					end
				end)
				L_415_.Name = L_413_forvar0
				L_415_.Parent = L_412_.Buttons -- saving memory
				L_415_.Size = UDim2.fromOffset(L_415_.TextLabel.TextBounds.X + 24, 30)
			end
		end
		
		L_8_:Create(L_22_, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
			BackgroundTransparency = .5
		}):Play()
		wait(.2)
		L_8_:Create(L_412_, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
			BackgroundTransparency = 0,
			Size = UDim2.new(0, 350, 0, 150)
		}):Play()
		wait(0.2)
		L_8_:Create(L_412_.UIStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
			Transparency = 0
		}):Play()
		L_8_:Create(L_412_.Title, TweenInfo.new(0.45, Enum.EasingStyle.Quint), {
			TextTransparency = 0
		}):Play()
		L_8_:Create(L_412_.Content, TweenInfo.new(0.45, Enum.EasingStyle.Quint), {
			TextTransparency = 0
		}):Play()
		L_8_:Create(L_412_.Sub, TweenInfo.new(0.45, Enum.EasingStyle.Quint), {
			TextTransparency = 0
		}):Play()
		wait(1)
		if L_411_arg0.Actions then
			for L_418_forvar0, L_419_forvar1 in pairs(L_412_.Buttons:GetChildren()) do
				if L_419_forvar1.Name ~= 'Template' and L_419_forvar1.Name ~= 'Middle' and L_419_forvar1:IsA('Frame') then
					L_419_forvar1.Visible = true
					L_8_:Create(L_419_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						Transparency = 0
					}):Play()
					L_8_:Create(L_419_forvar1.TextLabel, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
						TextTransparency = 0
					}):Play()
					wait(.8)
				end
			end
		else
			--TweenService:Create(PromptUI.Buttons.Middle.UIStroke,TweenInfo.new(0.3, Enum.EasingStyle.Quint), {Transparency = 0}):Play()
			--TweenService:Create(PromptUI.Buttons.Middle.TextLabel,TweenInfo.new(0.3, Enum.EasingStyle.Quint), {TextTransparency = 0}):Play()
		end
	end
	
	return L_217_
end


function L_6_:Destroy()
	L_13_:Destroy()
end

L_16_.ChangeSize.MouseButton1Click:Connect(function()
	if L_28_ then
		return
	end
	if L_26_ then
		L_26_ = false
		Maximise()
	else
		if not L_30_ then
			L_30_ = true
			spawn(CloseSearch)
		end
		L_26_ = true
		Minimise()
	end
end)
L_16_.Search.MouseButton1Click:Connect(function()
	if L_28_ or L_26_ then
		return
	end
	if L_30_ then
		OpenSearch()
		L_30_ = false
	else
		L_30_ = true
		CloseSearch()
	end
end)
L_16_.Hide.MouseButton1Click:Connect(function()
	if L_28_ then
		return
	end
	if L_27_ then
		L_27_ = false
		L_26_ = false
		Unhide()
	else
		if not L_30_ then
			L_30_ = true
			spawn(CloseSearch)
		end
		L_27_ = true
		Hide()
	end
end)

L_7_.InputBegan:Connect(function(L_420_arg0, L_421_arg1)
	if (L_420_arg0.KeyCode == Enum.KeyCode.RightShift and not L_421_arg1) then
		if L_28_ then
			return
		end
		if L_27_ then
			L_27_ = false
			Unhide()
		else
			if not L_30_ then
				spawn(CloseSearch)
			end
			L_27_ = true
			Hide()
		end
	end
end)

for L_422_forvar0, L_423_forvar1 in ipairs(L_16_:GetChildren()) do
	if L_423_forvar1.ClassName == "ImageButton" then
		L_423_forvar1.MouseEnter:Connect(function()
			L_8_:Create(L_423_forvar1, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				ImageTransparency = 0
			}):Play()
		end)

		L_423_forvar1.MouseLeave:Connect(function()
			L_8_:Create(L_423_forvar1, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				ImageTransparency = 0.8
			}):Play()
		end)

		L_423_forvar1.MouseButton1Click:Connect(function()
			L_8_:Create(L_423_forvar1, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				ImageTransparency = 0.8
			}):Play()
		end)
	end
end


function L_6_:LoadConfiguration()
	if L_25_ then
		pcall(function()
			if isfile(L_4_ .. "/" .. L_24_ .. L_5_) then
				L_36_func(readfile(L_4_ .. "/" .. L_24_ .. L_5_))
				L_6_:Notify({
					Title = "Configuration Loaded",
					Content = "The configuration file for this script has been loaded from a previous session"
				})
			end
		end)
	end
end

return L_6_
