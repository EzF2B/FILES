		ImageTransparency = 1
	}):Play()
	L_8_:Create(L_20_.Clear, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		ImageTransparency = 1
	}):Play()
	L_8_:Create(L_20_.UIStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		Transparency = 1
	}):Play()
	L_8_:Create(L_20_.Filter, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		ImageTransparency = 1
	}):Play()
	L_8_:Create(L_20_.Input, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
		TextTransparency = 1
	}):Play()
	delay(.3, function()
		L_20_.Input.Visible = false
	end)
	wait(0.5)
	L_20_.Visible = false
	L_28_ = false
end

function OpenSearch()
	L_28_ = true
	L_20_.Visible = true
	L_20_.Input.Visible = true
	L_8_:Create(L_20_, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 0,
		Size = UDim2.new(0, 480, 0, 40)
	}):Play()
	L_8_:Create(L_20_.Icon, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		ImageTransparency = 0.5
	}):Play()
	L_8_:Create(L_20_.UIStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		Transparency = 0
	}):Play()
	L_8_:Create(L_20_.Clear, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		ImageTransparency = .8
	}):Play()
	L_8_:Create(L_20_.Filter, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		ImageTransparency = .8
	}):Play()
	L_8_:Create(L_20_.Input, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
		TextTransparency = 0
	}):Play()
	wait(0.5)
	L_28_ = false
end
L_20_.Input:GetPropertyChangedSignal('Text'):Connect(function()
	local L_192_ = string.upper(L_20_.Input.Text)
	for L_193_forvar0, L_194_forvar1 in ipairs(L_17_:GetChildren()) do
		if L_194_forvar1 ~= 'Template' then
			for L_195_forvar0, L_196_forvar1 in pairs(L_194_forvar1:GetChildren()) do
				if L_196_forvar1:IsA("Frame") and L_196_forvar1.Name ~= 'Placeholder' and L_196_forvar1.Name ~= 'SectionSpacing' and not L_196_forvar1:GetAttribute('SectionTitle') then
					if L_192_ == "" or string.find(string.upper(L_196_forvar1.Name), L_192_) ~= nil then
						L_196_forvar1.Visible = true
					else
						L_196_forvar1.Visible = false
					end
				end
			end
		end
	end
end)
L_20_.Clear.MouseButton1Down:Connect(function()
	L_21_.Position = UDim2.new(0.957, 0, .5, 0)
	L_21_.Size = UDim2.new(0, 1, 0, 1)
	L_21_.BackgroundTransparency = .9

	local L_197_ = {}
	L_197_.Size = UDim2.new(0, 1000, 0, 500)
	L_197_.BackgroundTransparency = 1

	L_8_:Create(L_21_, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), L_197_):Play()
	L_20_.Input.Text = ''
end)

function Maximise()
	L_28_ = true
	L_16_.ChangeSize.Image = "rbxassetid://" .. 10137941941


	L_8_:Create(L_16_.UIStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Transparency = 1
	}):Play()
	L_8_:Create(L_15_.Shadow.Image, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		ImageTransparency = 0.4
	}):Play()
	L_8_:Create(L_16_.CornerRepair, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 0
	}):Play()
	L_8_:Create(L_16_.Divider, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 0
	}):Play()
	L_8_:Create(L_15_, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Size = UDim2.new(0, 500, 0, 475)
	}):Play()
	L_8_:Create(L_16_, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Size = UDim2.new(0, 500, 0, 45)
	}):Play()
	L_19_.Visible = true
	wait(0.2)

	L_17_.Visible = true

	for L_198_forvar0, L_199_forvar1 in ipairs(L_17_:GetChildren()) do
		if L_199_forvar1.Name ~= "Template" and L_199_forvar1.ClassName == "ScrollingFrame" and L_199_forvar1.Name ~= "Placeholder" then
			for L_200_forvar0, L_201_forvar1 in ipairs(L_199_forvar1:GetChildren()) do
				if L_201_forvar1.ClassName == "Frame" then
					if L_201_forvar1.Name ~= "SectionSpacing" and L_201_forvar1.Name ~= "Placeholder" then
						if L_201_forvar1.Name == "SectionTitle" then
							L_8_:Create(L_201_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								TextTransparency = 0
							}):Play()
						else
							L_8_:Create(L_201_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								BackgroundTransparency = 0
							}):Play()
							L_8_:Create(L_201_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								Transparency = 0
							}):Play()
							L_8_:Create(L_201_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								TextTransparency = 0
							}):Play()
						end
						for L_202_forvar0, L_203_forvar1 in ipairs(L_201_forvar1:GetChildren()) do
							if L_203_forvar1.Name ~= 'Description' and (L_203_forvar1.ClassName == "Frame" or L_203_forvar1.ClassName == "TextLabel" or L_203_forvar1.ClassName == "TextBox" or L_203_forvar1.ClassName == "ImageButton" or L_203_forvar1.ClassName == "ImageLabel") then
								L_203_forvar1.Visible = true
							end
						end
					end
				end
			end
		end
	end


	wait(0.1)

	for L_204_forvar0, L_205_forvar1 in ipairs(L_19_:GetChildren()) do
		if L_205_forvar1.ClassName == "Frame" and L_205_forvar1.Name ~= "Placeholder" then
			if tostring(L_17_.UIPageLayout.CurrentPage) == L_205_forvar1.Title.Text then
				L_8_:Create(L_205_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 0
				}):Play()
				L_8_:Create(L_205_forvar1.Image, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0
				}):Play()
				L_8_:Create(L_205_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					TextTransparency = 0
				}):Play()
				L_8_:Create(L_205_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					Transparency = 1
				}):Play()
				L_8_:Create(L_205_forvar1.Shadow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0.9
				}):Play()
			else
				L_8_:Create(L_205_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 0.7
				}):Play()
				L_8_:Create(L_205_forvar1.Shadow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0.7
				}):Play()
				L_8_:Create(L_205_forvar1.Image, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					ImageTransparency = 0.2
				}):Play()
				L_8_:Create(L_205_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					TextTransparency = 0.2
				}):Play()
				L_8_:Create(L_205_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
					Transparency = 0
				}):Play()
			end

		end
	end


	wait(0.5)
	L_28_ = false
end

function Minimise()
	L_28_ = true
	L_16_.ChangeSize.Image = "rbxassetid://" .. 11036884234

	for L_206_forvar0, L_207_forvar1 in ipairs(L_19_:GetChildren()) do
		if L_207_forvar1.ClassName == "Frame" and L_207_forvar1.Name ~= "Placeholder" then
			L_8_:Create(L_207_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 1
			}):Play()
			L_8_:Create(L_207_forvar1.Image, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				ImageTransparency = 1
			}):Play()
			L_8_:Create(L_207_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				TextTransparency = 1
			}):Play()
			L_8_:Create(L_207_forvar1.Shadow, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				ImageTransparency = 1
			}):Play()
			L_8_:Create(L_207_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				Transparency = 1
			}):Play()
		end
	end

	for L_208_forvar0, L_209_forvar1 in ipairs(L_17_:GetChildren()) do
		if L_209_forvar1.Name ~= "Template" and L_209_forvar1.ClassName == "ScrollingFrame" and L_209_forvar1.Name ~= "Placeholder" then
			for L_210_forvar0, L_211_forvar1 in ipairs(L_209_forvar1:GetChildren()) do
				if L_211_forvar1.ClassName == "Frame" then
					if L_211_forvar1.Name ~= "SectionSpacing" and L_211_forvar1.Name ~= "Placeholder" then
						if L_211_forvar1.Name == "SectionTitle" then
							L_8_:Create(L_211_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								TextTransparency = 1
							}):Play()
						else
							L_8_:Create(L_211_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								BackgroundTransparency = 1
							}):Play()
							L_8_:Create(L_211_forvar1.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								Transparency = 1
							}):Play()
							L_8_:Create(L_211_forvar1.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
								TextTransparency = 1
							}):Play()
						end
						for L_212_forvar0, L_213_forvar1 in ipairs(L_211_forvar1:GetChildren()) do
							if L_213_forvar1.ClassName == "Frame" or L_213_forvar1.ClassName == "TextLabel" or L_213_forvar1.ClassName == "TextBox" or L_213_forvar1.ClassName == "ImageButton" or L_213_forvar1.ClassName == "ImageLabel" then
								L_213_forvar1.Visible = false
							end
						end
					end
				end
			end
		end
	end

	L_8_:Create(L_16_.UIStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Transparency = 0
	}):Play()
	L_8_:Create(L_15_.Shadow.Image, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		ImageTransparency = 1
	}):Play()
	L_8_:Create(L_16_.CornerRepair, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 1
	}):Play()
	L_8_:Create(L_16_.Divider, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 1
	}):Play()
	L_8_:Create(L_15_, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Size = UDim2.new(0, 495, 0, 45)
	}):Play()
	L_8_:Create(L_16_, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Size = UDim2.new(0, 495, 0, 45)
	}):Play()

	wait(0.3)

	L_17_.Visible = false
	L_19_.Visible = false

	wait(0.2)
	L_28_ = false
end

function L_6_:CreateWindow(L_214_arg0)
	local L_215_ = false
	L_16_.Title.Text = L_214_arg0.Name
	L_15_.Size = UDim2.new(0, 450, 0, 260)
	L_15_.Visible = true
	L_15_.BackgroundTransparency = 1
	L_18_.Title.TextTransparency = 1
	L_18_.Subtitle.TextTransparency = 1
	L_15_.Shadow.Image.ImageTransparency = 1
	L_18_.Version.TextTransparency = 1
	L_18_.Title.Text = L_214_arg0.LoadingTitle or "Rayfield Interface Suite"
	L_18_.Subtitle.Text = L_214_arg0.LoadingSubtitle or "by Sirius"
	if L_214_arg0.LoadingTitle ~= "Rayfield Interface Suite" then
		L_18_.Version.Text = "F2B"
	end
	L_16_.Visible = false
	L_17_.Visible = false
	L_18_.Visible = true


	pcall(function()
		if not L_214_arg0.ConfigurationSaving.FileName then
			L_214_arg0.ConfigurationSaving.FileName = tostring(game.PlaceId)
		end
		if not isfolder(L_3_ .. "/Configuration Folders") then

		end
		if L_214_arg0.ConfigurationSaving.Enabled == nil then
			L_214_arg0.ConfigurationSaving.Enabled = false
		end
		L_24_ = L_214_arg0.ConfigurationSaving.FileName
		L_4_ = L_214_arg0.ConfigurationSaving.FolderName or L_4_
		L_25_ = L_214_arg0.ConfigurationSaving.Enabled

		if L_214_arg0.ConfigurationSaving.Enabled then
			if not isfolder(L_4_) then
				makefolder(L_4_)
			end	
		end
	end)

	L_33_func(L_16_, L_15_)

	for L_218_forvar0, L_219_forvar1 in ipairs(L_19_:GetChildren()) do
		if L_219_forvar1.ClassName == "Frame" and L_219_forvar1.Name ~= "Placeholder" then
			L_219_forvar1.BackgroundTransparency = 1
			L_219_forvar1.Title.TextTransparency = 1
			L_219_forvar1.Shadow.ImageTransparency = 1
			L_219_forvar1.Image.ImageTransparency = 1
			L_219_forvar1.UIStroke.Transparency = 1
		end
	end

	if L_214_arg0.Discord then
		if not isfolder(L_3_ .. "/Discord Invites") then
			makefolder(L_3_ .. "/Discord Invites")
		end
		if not isfile(L_3_ .. "/Discord Invites" .. "/" .. L_214_arg0.Discord.Invite .. L_5_) then
			if L_23_ then
				L_23_({
					Url = 'http://127.0.0.1:6463/rpc?v=1',
					Method = 'POST',
					Headers = {
						['Content-Type'] = 'application/json',
						Origin = 'https://discord.com'
					},
					Body = L_9_:JSONEncode({
						cmd = 'INVITE_BROWSER',
						nonce = L_9_:GenerateGUID(false),
						args = {
							code = L_214_arg0.Discord.Invite
						}
					})
				})
			end

			if L_214_arg0.Discord.RememberJoins then -- We do logic this way so if the developer changes this setting, the user still won't be prompted, only new users
				writefile(L_3_ .. "/Discord Invites" .. "/" .. L_214_arg0.Discord.Invite .. L_5_, "Rayfield RememberJoins is true for this invite, this invite will not ask you to join again")
			end
		else

		end
	end

	if L_214_arg0.KeySystem then
		if not L_214_arg0.KeySettings then
			L_215_ = true
			return
		end

		if not isfolder(L_3_ .. "/Key System") then
			makefolder(L_3_ .. "/Key System")
		end

		if L_214_arg0.KeySettings.GrabKeyFromSite then
			local L_220_, L_221_ = pcall(function()
				L_214_arg0.KeySettings.Key = game:HttpGet(L_214_arg0.KeySettings.Key)
			end)
			if not L_220_ then
				print("Rayfield | " .. L_214_arg0.KeySettings.Key .. " Error " .. tostring(L_221_))
			end
		end

		if not L_214_arg0.KeySettings.FileName then
			L_214_arg0.KeySettings.FileName = "No file name specified"
		end

		if isfile(L_3_ .. "/Key System" .. "/" .. L_214_arg0.KeySettings.FileName .. L_5_) then
			if readfile(L_3_ .. "/Key System" .. "/" .. L_214_arg0.KeySettings.FileName .. L_5_) == L_214_arg0.KeySettings.Key then
				L_215_ = true
			end
		end

		if not L_215_ then
			local L_222_ = math.random(2, 6)
			L_13_.Enabled = false
			local L_223_ = game:GetObjects("rbxassetid://11695805160")[1]
			L_223_.Enabled = true
			if gethui then
				L_223_.Parent = gethui()
			elseif syn.protect_gui then
				syn.protect_gui(L_13_)
				L_223_.Parent = L_12_
			else
				L_223_.Parent = L_12_
			end

			if gethui then
				for L_225_forvar0, L_226_forvar1 in ipairs(gethui():GetChildren()) do
					if L_226_forvar1.Name == L_223_.Name and L_226_forvar1 ~= L_223_ then
						L_226_forvar1.Enabled = false
						L_226_forvar1.Name = "KeyUI-Old"
					end
				end
			else
				for L_227_forvar0, L_228_forvar1 in ipairs(L_12_:GetChildren()) do
					if L_228_forvar1.Name == L_223_.Name and L_228_forvar1 ~= L_223_ then
						L_228_forvar1.Enabled = false
						L_228_forvar1.Name = "KeyUI-Old"
					end
				end
			end

			local L_224_ = L_223_.Main
			L_224_.Title.Text = L_214_arg0.KeySettings.Title or L_214_arg0.Name
			L_224_.Subtitle.Text = L_214_arg0.KeySettings.Subtitle or "Key System"
			L_224_.NoteMessage.Text = L_214_arg0.KeySettings.Note or "No instructions"

			L_224_.Size = UDim2.new(0, 467, 0, 175)
			L_224_.BackgroundTransparency = 1
			L_224_.EShadow.ImageTransparency = 1
			L_224_.Title.TextTransparency = 1
			L_224_.Subtitle.TextTransparency = 1
			L_224_.KeyNote.TextTransparency = 1
			L_224_.Input.BackgroundTransparency = 1
			L_224_.Input.UIStroke.Transparency = 1
			L_224_.Input.InputBox.TextTransparency = 1
			L_224_.NoteTitle.TextTransparency = 1
			L_224_.NoteMessage.TextTransparency = 1
			L_224_.Hide.ImageTransparency = 1

			L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 0
			}):Play()
			L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
				Size = UDim2.new(0, 500, 0, 187)
			}):Play()
			L_8_:Create(L_224_.EShadow, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
				ImageTransparency = 0.5
			}):Play()
			wait(0.05)
			L_8_:Create(L_224_.Title, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
				TextTransparency = 0
			}):Play()
			L_8_:Create(L_224_.Subtitle, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
				TextTransparency = 0
			}):Play()
			wait(0.05)
			L_8_:Create(L_224_.KeyNote, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
				TextTransparency = 0
			}):Play()
			L_8_:Create(L_224_.Input, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 0
			}):Play()
			L_8_:Create(L_224_.Input.UIStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
				Transparency = 0
			}):Play()
			L_8_:Create(L_224_.Input.InputBox, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
				TextTransparency = 0
			}):Play()
			wait(0.05)
			L_8_:Create(L_224_.NoteTitle, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
				TextTransparency = 0
			}):Play()
			L_8_:Create(L_224_.NoteMessage, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
				TextTransparency = 0
			}):Play()
			wait(0.15)
			L_8_:Create(L_224_.Hide, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
				ImageTransparency = 0.3
			}):Play()


			L_223_.Main.Input.InputBox.FocusLost:Connect(function()
				if L_224_.Input.InputBox.Text == L_214_arg0.KeySettings.Key then
					L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						BackgroundTransparency = 1
					}):Play()
					L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						Size = UDim2.new(0, 467, 0, 175)
					}):Play()
					L_8_:Create(L_224_.EShadow, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
						ImageTransparency = 1
					}):Play()
					L_8_:Create(L_224_.Title, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
						TextTransparency = 1
					}):Play()
					L_8_:Create(L_224_.Subtitle, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
						TextTransparency = 1
					}):Play()
					L_8_:Create(L_224_.KeyNote, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
						TextTransparency = 1
					}):Play()
					L_8_:Create(L_224_.Input, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
						BackgroundTransparency = 1
					}):Play()
					L_8_:Create(L_224_.Input.UIStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
						Transparency = 1
					}):Play()
					L_8_:Create(L_224_.Input.InputBox, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
						TextTransparency = 1
					}):Play()
					L_8_:Create(L_224_.NoteTitle, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
						TextTransparency = 1
					}):Play()
					L_8_:Create(L_224_.NoteMessage, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
						TextTransparency = 1
					}):Play()
					L_8_:Create(L_224_.Hide, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
						ImageTransparency = 1
					}):Play()
					delay(.4, function()
						L_224_.Hide.Visible = false
					end)
					wait(0.51)
					L_215_ = true
					if L_214_arg0.KeySettings.SaveKey then
						if writefile then
							writefile(L_3_ .. "/Key System" .. "/" .. L_214_arg0.KeySettings.FileName .. L_5_, L_214_arg0.KeySettings.Key)
						end
						L_6_:Notify({
							Title = "Key System",
							Content = "The key for this script has been saved successfully"
						})
					end
				else
					if L_222_ == 0 then
						L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
							BackgroundTransparency = 1
						}):Play()
						L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
							Size = UDim2.new(0, 467, 0, 175)
						}):Play()
						L_8_:Create(L_224_.Shadow.Image, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
							ImageTransparency = 1
						}):Play()
						L_8_:Create(L_224_.Title, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
							TextTransparency = 1
						}):Play()
						L_8_:Create(L_224_.Subtitle, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
							TextTransparency = 1
						}):Play()
						L_8_:Create(L_224_.KeyNote, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
							TextTransparency = 1
						}):Play()
						L_8_:Create(L_224_.Input, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
							BackgroundTransparency = 1
						}):Play()
						L_8_:Create(L_224_.Input.UIStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
							Transparency = 1
						}):Play()
						L_8_:Create(L_224_.Input.InputBox, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
							TextTransparency = 1
						}):Play()
						L_8_:Create(L_224_.NoteTitle, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
							TextTransparency = 1
						}):Play()
						L_8_:Create(L_224_.NoteMessage, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
							TextTransparency = 1
						}):Play()
						L_8_:Create(L_224_.Hide, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
							ImageTransparency = 1
						}):Play()
						wait(0.45)
						game.Players.LocalPlayer:Kick("No Attempts Remaining")
						game:Shutdown()
					end
					L_224_.Input.InputBox.Text = ""
					L_222_ = L_222_ - 1
					L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						Size = UDim2.new(0, 467, 0, 175)
					}):Play()
					L_8_:Create(L_224_, TweenInfo.new(0.4, Enum.EasingStyle.Elastic), {
						Position = UDim2.new(0.495, 0, 0.5, 0)
					}):Play()
					wait(0.1)
					L_8_:Create(L_224_, TweenInfo.new(0.4, Enum.EasingStyle.Elastic), {
						Position = UDim2.new(0.505, 0, 0.5, 0)
					}):Play()
					wait(0.1)
					L_8_:Create(L_224_, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
						Position = UDim2.new(0.5, 0, 0.5, 0)
					}):Play()
					L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
						Size = UDim2.new(0, 500, 0, 187)
					}):Play()
				end
			end)

			L_224_.Hide.MouseButton1Click:Connect(function()
				L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
					BackgroundTransparency = 1
				}):Play()
				L_8_:Create(L_224_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
					Size = UDim2.new(0, 467, 0, 175)
				}):Play()
				L_8_:Create(L_224_.EShadow, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
					ImageTransparency = 1
				}):Play()
				L_8_:Create(L_224_.Title, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
					TextTransparency = 1
				}):Play()
				L_8_:Create(L_224_.Subtitle, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
					TextTransparency = 1
				}):Play()
