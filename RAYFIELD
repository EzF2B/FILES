		BackgroundTransparency = 1
	}):Play()
	L_8_:Create(L_15_.Topbar.CornerRepair, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
		BackgroundTransparency = 1
	}):Play()
	L_8_:Create(L_15_.Topbar.Title, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		TextTransparency = 1
	}):Play()
	L_8_:Create(L_15_.Shadow.Image, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		ImageTransparency = 1
	}):Play()
	L_8_:Create(L_16_.UIStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
		Transparency = 1
	}):Play()
	for L_172_forvar0, L_173_forvar1 in ipairs(L_16_:GetChildren()) do
		if L_173_forvar1.ClassName == "ImageButton" then
			L_8_:Create(L_173_forvar1, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {
				ImageTransparency = 1
			}):Play()
		end
	end
	for L_174_forvar0, L_175_forvar1 in ipairs(L_19_:GetChildren()) do
		if L_175_forvar1.ClassName == "Frame" and L_175_forvar1.Name ~= "Placeholder" then
			L_8_:Create(L_175_forvar1, TweenInfo.new(0.3, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 1
