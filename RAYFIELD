			L_321_.KeybindFrame.UIStroke.Color = L_32_.InputStroke

			L_8_:Create(L_321_, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				BackgroundTransparency = 0
			}):Play()
			L_8_:Create(L_321_.UIStroke, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				Transparency = 0
			}):Play()
			L_8_:Create(L_321_.Title, TweenInfo.new(0.7, Enum.EasingStyle.Quint), {
				TextTransparency = 0
			}):Play()	
			
			if L_319_arg0.Info then
				L_321_.Description.Text = L_319_arg0.Info
				local L_322_ = false
				local L_323_ = false
				local L_324_ = L_321_.Size.Y.Offset

				L_321_.MouseEnter:Connect(function(L_325_arg0, L_326_arg1)
					L_322_ = true
					wait(0.2)
					if L_322_ then
						L_323_ = true
						L_321_.Description.Visible = true
						game:GetService('TweenService'):Create(L_321_, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
							Size = UDim2.new(1, -10, 0, L_324_ + L_321_.Description.Size.Y.Offset + 15)
						}):Play()
					end
				end)
				L_321_.MouseLeave:Connect(function(L_327_arg0, L_328_arg1)
					if L_322_ then
						L_322_ = false
					end
					if L_323_ then
						L_323_ = false
						game:GetService('TweenService'):Create(L_321_, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
							Size = UDim2.new(1, -10, 0, L_324_)
						}):Play()
						delay(.4, function()
							if not L_323_ then
								L_321_.Description.Visible = false
							end
						end)
					end
				end)
			end
			
			L_321_.KeybindFrame.KeybindBox.Text = L_319_arg0.CurrentKeybind
			L_321_.KeybindFrame.Size = UDim2.new(0, L_321_.KeybindFrame.KeybindBox.TextBounds.X + 24, 0, 30)

			L_321_.KeybindFrame.KeybindBox.Focused:Connect(function()
				L_320_ = true
				L_321_.KeybindFrame.KeybindBox.Text = ""
			end)
			L_321_.KeybindFrame.KeybindBox.FocusLost:Connect(function()
				L_320_ = false
				if L_321_.KeybindFrame.KeybindBox.Text == nil or "" then
					L_321_.KeybindFrame.KeybindBox.Text = L_319_arg0.CurrentKeybind
					L_37_func()
				end
			end)

			L_321_.MouseEnter:Connect(function()
				L_8_:Create(L_321_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
					BackgroundColor3 = L_32_.ElementBackgroundHover
				}):Play()
			end)

			L_321_.MouseLeave:Connect(function()
				L_8_:Create(L_321_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
					BackgroundColor3 = L_32_.ElementBackground
				}):Play()
			end)

			L_7_.InputBegan:Connect(function(L_329_arg0, L_330_arg1)

				if L_320_ then
					if L_329_arg0.KeyCode ~= Enum.KeyCode.Unknown and L_329_arg0.KeyCode ~= Enum.KeyCode.RightShift then
						local L_331_ = string.split(tostring(L_329_arg0.KeyCode), ".")
						local L_332_ = L_331_[3]
						L_321_.KeybindFrame.KeybindBox.Text = tostring(L_332_)
						L_319_arg0.CurrentKeybind = tostring(L_332_)
						L_321_.KeybindFrame.KeybindBox:ReleaseFocus()
						L_37_func()
					end
				elseif L_319_arg0.CurrentKeybind ~= nil and (L_329_arg0.KeyCode == Enum.KeyCode[L_319_arg0.CurrentKeybind] and not L_330_arg1) then  --Test
					local L_333_ = true
					local L_334_
					L_334_ = L_329_arg0.Changed:Connect(function(L_335_arg0)
						if L_335_arg0 == "UserInputState" then
							L_334_:Disconnect()
							L_333_ = false
						end
					end)

					if not L_319_arg0.HoldToInteract then
						local L_336_, L_337_ = pcall(L_319_arg0.Callback)
						if not L_336_ then
							L_8_:Create(L_321_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
								BackgroundColor3 = Color3.fromRGB(85, 0, 0)
							}):Play()
							L_8_:Create(L_321_.UIStroke, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
								Transparency = 1
							}):Play()
							L_321_.Title.Text = "Callback Error"
							print("Rayfield | " .. L_319_arg0.Name .. " Callback Error " .. tostring(L_337_))
							wait(0.5)
							L_321_.Title.Text = L_319_arg0.Name
							L_8_:Create(L_321_, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
								BackgroundColor3 = L_32_.ElementBackground
							}):Play()
							L_8_:Create(L_321_.UIStroke, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
								Transparency = 0
							}):Play()
						end
					else
						wait(0.25)
						if L_333_ then
							local L_338_;
							L_338_ = L_10_.Stepped:Connect(function()
								if not L_333_ then
									L_319_arg0.Callback(false) -- maybe pcall this
									L_338_:Disconnect()
								else
									L_319_arg0.Callback(true) -- maybe pcall this
								end
							end)	
						end
					end
				end
			end)

			L_321_.KeybindFrame.KeybindBox:GetPropertyChangedSignal("Text"):Connect(function()
				L_8_:Create(L_321_.KeybindFrame, TweenInfo.new(0.55, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
